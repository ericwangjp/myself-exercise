<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.my.mywebview.activity.customview.CustomView3Activity">
    <include
        android:id="@+id/title"
        layout="@layout/title_common" />
    <com.my.mywebview.view.customview.CustomVolumControlBar
        android:layout_width="@dimen/dimens300dp"
        android:layout_height="@dimen/dimens300dp"
        android:padding="@dimen/dimens10dp"
        android:layout_margin="@dimen/dimens10dp"
        android:layout_gravity="center_horizontal"
        custom:circleWidth="@dimen/dimens20dp"
        custom:firstColor="@color/colorF1BB61"
        custom:secondColor="@color/color2DC868"
        custom:splitSize="20"
        custom:dotCount="12"
        custom:bg="@drawable/money01"/>
    <com.my.mywebview.view.customview.CustomVolumControlBar
        android:layout_width="@dimen/dimens200dp"
        android:layout_height="@dimen/dimens200dp"
        android:padding="@dimen/dimens10dp"
        android:layout_margin="@dimen/dimens10dp"
        android:layout_gravity="center_horizontal"
        custom:circleWidth="@dimen/dimens20dp"
        custom:firstColor="@color/color1398FF"
        custom:secondColor="@color/colorDB4E43"
        custom:splitSize="20"
        custom:dotCount="8"
        custom:bg="@drawable/icon_gold"/>
</LinearLayout>
