<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.my.mywebview.activity.customview.CustomView2Activity">
    <include
        android:id="@+id/title"
        layout="@layout/title_common" />
    <com.my.mywebview.view.customview.CustomProgressBar
        android:id="@+id/customPB"
        android:layout_width="@dimen/dimens200dp"
        android:layout_height="@dimen/dimens200dp"
        android:layout_gravity="center_horizontal"
        android:padding="@dimen/dimens10dp"
        android:layout_margin="@dimen/dimens10dp"
        custom:firstColor="@color/color2DC868"
        custom:secondColor="@color/colorEC4949"
        custom:circleWidth="@dimen/dimens40dp"
        custom:speed="20"
        custom:txtColor="@color/colorFB894C"
        custom:txtContent="当前进度%1$d%%"
        custom:txtSize="@dimen/dimens16sp"/>
</LinearLayout>
