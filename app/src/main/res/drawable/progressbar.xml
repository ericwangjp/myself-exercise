<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
  
    <item android:id="@android:id/background">  
  
        <shape>  
  
            <corners android:radius="5dip" />  
  
            <gradient  
                android:angle="0"  
                android:centerColor="#fce5cd"  
                android:centerY="0.75"  
                android:endColor="#fce5cd"  
                android:startColor="#fce5cd" />  
        </shape>  
    </item>  
  
    <item android:id="@android:id/secondaryProgress">  
  
        <clip>  
  
            <shape>  
  
                <corners android:radius="5dip" />  
  
                <gradient  
                    android:angle="0"  
                    android:centerColor="#9fc5e8"  
                    android:centerY="0.75"  
                    android:endColor="#da3bec"  
                    android:startColor="#80ffd300" />  
            </shape>  
        </clip>  
    </item>  
  
    <item android:id="@android:id/progress">  
  
        <clip>  
  
            <shape>  
  
                <corners android:radius="5dip" />  
  
                <gradient  
                    android:angle="0"  
                    android:endColor="#3c78d8"  
                    android:startColor="#6d9eeb" />  
            </shape>  
        </clip>  
    </item>  
  
</layer-list>
